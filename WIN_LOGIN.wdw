  procedures :
   -
     name : CreateAccount
     procedure_id : 1165648106888031801
     type_code : 14
     code : |1-
      
      // Summary: Creates a user account
      //
      procedure CreateAccount()
      
      HReadSeekFirst(User,Login,EDT_Username_Crea)
      if not HFound(User) then
      	// Create the account
      	User.Login = EDT_Username_Crea
      	User.Password = HashString(HA_HMAC_SHA_160,EDT_Password_Crea)
      HAdd(User)
      	
      	//////////////////////////////////////////////////
      	// Generate a key in base 32 that will be used for the double authentication
      	User.KeyTOTP = TwoFactorAuthenticationGenerateTOTPKey(User.ForumUserID + "-" + GetGUID(guidRough))
      	
      HModify(User)
      	
      gnConnectedUserID = User.ForumUserID
      	
      	return True
      else
      ToastDisplay("This user already exists",toastShort,vaTop,haCenter,LightRed)
      	return False
      end
