  type_code : 1
  p_codes :
   -
     code : |1+
      procedure MyWindow()
      
      if gnConnectedUserID <= 0 then Disconnection()
      
      // Find the user
      HReadSeekFirst(User,ForumUserID,gnConnectedUserID)
      
      if not HFound(User) then Disconnection()
      
      
      // User data
      STC_Username = User.Login
      
      // Display information about the double authentication
      sLink is string 
      
      sLink									= TwoFactorAuthenticationGenerateLink(User.KeyTOTP,User.Login,"WD Two-factor Authentication")
      
      BCOD_Link								= sLink
      STC_Key									= User.KeyTOTP
      CBOX_2FA								= User.TwoFAEnabled
      GR_2FA.Visible							= User.TwoFAEnabled
      
